@model RutasCheck.Models.Concecionario

@{
    ViewData["Title"] = "Delete";
     ViewData["header-title-page"] = "Concecionarios/Propietarios";
    ViewData["header-sub-title-page"] = "Elimniar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="page-inner">
    <div class="card">
        <div class="card-body">
            <h3>¿Estás seguro que deseas eliminar?</h3>

            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Nombre)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Nombre)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="IdConcecionario" />
                <a class="btn btn-sm btn-primary" asp-action="Index">Cancelar</a>
                <input type="submit" value="Eliminar" class="btn btn-sm btn-danger" />
            </form>
        </div>
    </div>
</div>
